{
  "name": "joelwmale/laravel-cart",
  "version": "1.2.0",
  "description": "A cart implementation for Laravel",
  "keywords": [
    "laravel",
    "shopping cart",
    "cart"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Darryl Fernandez",
      "email": "engrdarrylfernandez@gmail.com"
    },
    {
      "name": "Joel Male",
      "email": "joel@joelmale.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/auth": "^10.0|^11.0|^12.0",
    "illuminate/container": "^10.0|^11.0|^12.0",
    "illuminate/database": "^10.0|^11.0|^12.0",
    "illuminate/validation": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.6",
    "laravel/pint": "^1.21",
    "pestphp/pest": "^3.0",
    "larapack/dd": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Joelwmale\\Cart\\": "src/"
    }
  },
  "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "Joelwmale\\Cart\\CartServiceProvider"
      ],
      "aliases": {
        "Cart": "Joelwmale\\Cart\\Facades\\CartFacade"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
